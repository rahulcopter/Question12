public class HighlyDivisibleTriangularNumber {

	public static void main(String[] args) {
	
		int[] nthTriangularNumber = new int [100000];
		//triangularNumbers [1] = 1;
		//triangularNumbers [2] = 1 + 2;
		//triangularNumbers [3] = 1 + 2 + 3;
		int n = 0;
		nthTriangularNumber [n] = n + (n-1) + (n-2);
	}

}
